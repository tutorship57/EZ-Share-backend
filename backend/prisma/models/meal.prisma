model Meal{
    meal_id String @id @default(uuid())
    meal_name String
    meal_at String
    meal_status String
    meal_host String
    guest_host Guest @relation(fields: [meal_host],references: [guest_id])
    trip_id String
    trip_table Trip @relation(fields: [trip_id],references: [trip_id])
    menuList Menu[]
    mealShareList MenuShare[]
}